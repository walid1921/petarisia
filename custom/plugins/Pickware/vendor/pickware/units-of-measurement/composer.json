{
    "name": "pickware/units-of-measurement",
    "description": "A library that provides functionality for dealing with units of measurement such as length and weight.",
    "license": "proprietary",
    "type": "library",
    "version": "3.12.0",
    "authors": [
        {
            "name": "Pickware GmbH",
            "email": "info@pickware.de"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "2.1.29",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Pickware\\UnitsOfMeasurement\\": "src"
        },
        "exclude-from-classmap": [
            "**.unit.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true,
            "viison/composer-git-hooks-installer-plugin": true
        },
        "lock": false
    },
    "extra": {
        "ci-min-supported-shopware-version": "6.6.10.5"
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=4G --no-interaction --ansi -c phpstan.neon.dist",
        "test:unit": "composer install && ./vendor/bin/phpunit --config phpunit.unit.xml --testdox"
    }
}
