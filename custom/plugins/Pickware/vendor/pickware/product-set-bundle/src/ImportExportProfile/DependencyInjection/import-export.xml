<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <parameters>
        <parameter key="pickware_product_set.import_export_profiles.product-set.batch_size">50</parameter>
    </parameters>
    <services>
        <service id="Pickware\ProductSetBundle\ImportExportProfile\ProductSetConfigurationCsvRowNormalizer">

        </service>
        <service id="Pickware\ProductSetBundle\ImportExportProfile\ProductSetConfigurationImporter">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
            <argument
                type="service"
                id="Pickware\ProductSetBundle\ImportExportProfile\ProductSetConfigurationCsvRowNormalizer"
            />
            <argument type="service" id="Pickware\PickwareErpStarter\ImportExport\ImportExportStateService" />
            <argument type="service" id="Pickware\ValidationBundle\JsonValidator" />
            <argument>%pickware_product_set.import_export_profiles.product-set.batch_size%</argument>
            <tag name="pickware_erp.import_export.importer" profileTechnicalName="product-set-configuration" />
        </service>
        <service id="Pickware\ProductSetBundle\ImportExportProfile\ProductSetConfigurationExporter">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
            <argument type="service" id="Pickware\PickwareErpStarter\Translation\Translator" />
            <argument type="service" id="Pickware\DalBundle\CriteriaJsonSerializer" />
            <argument type="service" id="Pickware\ShopwareExtensionsBundle\Product\ProductNameFormatterService" />
            <argument>%pickware_product_set.import_export_profiles.product-set.batch_size%</argument>
            <tag name="pickware_erp.import_export.exporter" profileTechnicalName="product-set-configuration" />
        </service>
    </services>
</container>
