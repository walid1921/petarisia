{
    "name": "pickware/shipping-bundle",
    "description": "A bundle that contains functionality that is shared by our shipping plugins.",
    "license": "proprietary",
    "type": "library",
    "version": "3.116.0",
    "authors": [
        {
            "name": "Pickware GmbH",
            "email": "info@pickware.de"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "ext-filter": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-soap": "*",
        "doctrine/dbal": "^3.6 || ^4.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "guzzlehttp/promises": "^1.5 || ^2.0",
        "guzzlehttp/psr7": "^2.4.5",
        "league/flysystem": "^3.10.3",
        "monolog/monolog": "^3.3.1",
        "pickware/api-error-handling-bundle": "^3.82.0",
        "pickware/api-versioning-bundle": "^3.31.0",
        "pickware/bundle-installer": "^3.15.0",
        "pickware/dal-bundle": "^5.66.0",
        "pickware/document-bundle": "^4.99.0",
        "pickware/feature-flag-bundle": "^3.75.0",
        "pickware/http-utils": "^4.36.0",
        "pickware/installation-library": "^4.76.0",
        "pickware/money-bundle": "^4.66.0",
        "pickware/php-standard-library": "^2.29.0",
        "pickware/shopware-extensions-bundle": "^3.72.0",
        "pickware/units-of-measurement": "^3.12.0",
        "pickware/validation-bundle": "^3.80.0",
        "psr/http-message": "^1.1 || ^2.0",
        "psr/log": "^3.0.0",
        "shopware/core": ">=6.6.10.5 <6.8",
        "symfony/config": "^7.0.0",
        "symfony/console": "^7.0.0",
        "symfony/dependency-injection": "^7.0.0",
        "symfony/event-dispatcher": "^7.0.0",
        "symfony/http-foundation": "^7.0.0",
        "symfony/yaml": "^7.0.0",
        "viison/address-splitter": "^0.3.2"
    },
    "require-dev": {
        "league/flysystem-memory": "~3.13.0",
        "mikey179/vfsstream": "^1.6",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "2.1.29",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5.0",
        "pickware/phpstan-extensions": "*@dev",
        "pickware/unit-testing-tools": "*@dev",
        "shopware/storefront": ">=6.6.10.5 <6.8",
        "swiftmailer/swiftmailer": "^6.2.2"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../libraries/composer-packages/*"
        },
        {
            "type": "path",
            "url": "../../libraries-dev/composer-packages/*"
        },
        {
            "type": "path",
            "url": "../../bundles/*"
        },
        {
            "type": "path",
            "url": "../../plugins-dev/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pickware\\ShippingBundle\\": "src",
            "Pickware\\ShippingBundle\\Test\\TestEntityCreation\\": "test/TestEntityCreation/"
        },
        "exclude-from-classmap": [
            "**.unit.php",
            "**.integration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pickware\\ShippingBundle\\Test\\": "test"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "pickware/dependency-loader": true,
            "symfony/runtime": true,
            "viison/composer-git-hooks-installer-plugin": true
        },
        "lock": false
    },
    "extra": {
        "ci-min-supported-shopware-version": "6.6.10.5"
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=4G --no-interaction --ansi -c phpstan.neon.dist",
        "test:integration": "${SHOPWARE_DIR}/vendor/bin/phpunit --config phpunit.integration.xml --testdox",
        "test:unit": "composer install && ./vendor/bin/phpunit --config phpunit.unit.xml --testdox"
    }
}
