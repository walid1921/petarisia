<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <instanceof id="Pickware\ShippingBundle\AddressCorrecting\AddressCorrecting" autowire="true">
            <tag name="pickware_shipping_bundle.address_correcting" />
        </instanceof>
        <service id="Pickware\ShippingBundle\AddressCorrecting\AddressCorrectingService">
            <argument type="tagged_iterator" tag="pickware_shipping_bundle.address_correcting" />
        </service>
    </services>
</container>
