<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="Pickware\ShippingBundle\Privacy\ConfirmDataTransferInStorefrontSubscriber">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="Pickware\ShippingBundle\Privacy\DataTransferAskCustomerPolicyFeatureFlag">
            <tag name="pickware_feature_flag.feature_flag" />
        </service>
        <service id="Pickware\ShippingBundle\Privacy\PrivacyService">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
            <argument type="service" id="Pickware\FeatureFlagBundle\FeatureFlagService" />
        </service>
    </services>
</container>
