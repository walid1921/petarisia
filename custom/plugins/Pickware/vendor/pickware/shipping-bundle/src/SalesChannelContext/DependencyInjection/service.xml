<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="Pickware\ShippingBundle\SalesChannelContext\ScheduledTask\SalesChannelContextCleanupTask">
            <tag name="shopware.scheduled.task" />
        </service>
        <service id="Pickware\ShippingBundle\SalesChannelContext\ScheduledTask\SalesChannelContextCleanupTaskHandler">
            <argument type="service" id="scheduled_task.repository" />
            <argument type="service" id="Doctrine\DBAL\Connection" />
            <argument type="service" id="logger" />
            <tag name="messenger.message_handler" />
        </service>
        <service id="Pickware\ShippingBundle\SalesChannelContext\Subscriber\SynchronizeSalesChannelContextsSubscriber">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
