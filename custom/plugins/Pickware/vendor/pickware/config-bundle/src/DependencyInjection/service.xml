<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="Pickware\ConfigBundle\ScheduledTaskExecutionTimeUpdater">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
        </service>
        <service id="Pickware\ConfigBundle\ScheduledTaskConfigSubscriber">
            <argument type="tagged_iterator" tag="pickware_config_bundle.configurable_scheduled_task" />
            <argument type="service" id="Doctrine\DBAL\Connection" />
            <argument type="service" id="Psr\Clock\ClockInterface" />
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
