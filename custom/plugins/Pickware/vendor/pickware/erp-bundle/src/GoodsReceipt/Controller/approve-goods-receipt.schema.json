{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": ["goodsReceiptId"],
    "additionalProperties": false,
    "properties": {
        "goodsReceiptId": {
            "$ref": "#/definitions/uuid"
        }
    },
    "definitions": {
        "uuid": {
            "description": "A UUID in HEX format",
            "type": "string",
            "minLength": 32,
            "maxLength": 32,
            "pattern": "^[0-9a-f]{32}$"
        }
    }
}
