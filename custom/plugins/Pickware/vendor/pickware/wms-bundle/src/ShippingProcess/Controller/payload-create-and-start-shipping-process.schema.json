{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Shipping process create-and-start-shipping-process payload",
    "description": "PUT request payload for api route _action/pickware-wms/create-and-start-shipping-process",
    "type": "object",
    "required": ["associations", "shippingProcess"],
    "properties": {
        "associations": {
            "type": "object"
        },
        "shippingProcess": {
            "$ref": "#/definitions/shippingProcess"
        }
    },
    "definitions": {
        "shippingProcess": {
            "type": "object",
            "required": ["id", "warehouseId", "pickingProcesses"],
            "properties": {
                "id": {
                    "$ref": "#/definitions/uuid"
                },
                "warehouseId": {
                    "$ref": "#/definitions/uuid"
                },
                "pickingProcesses": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/pickingProcess"
                    },
                    "minItems": 1
                }
            }
        },
        "pickingProcess": {
            "type": "object",
            "required": ["id"],
            "properties": {
                "id": {
                    "$ref": "#/definitions/uuid"
                }
            }
        },
        "uuid": {
            "description": "A UUID in HEX format",
            "type": "string",
            "minLength": 32,
            "maxLength": 32,
            "pattern": "^[0-9a-f]{32}$"
        }
    }
}
