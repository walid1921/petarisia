{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Picking process cancel-picking-process payload",
    "description": "POST request payload for api route _action/pickware-wms/cancel-picking-process",
    "type": "object",
    "required": ["pickingProcessId", "associations", "stockReversionAction"],
    "properties": {
        "pickingProcessId": {
            "$ref": "#/definitions/uuid"
        },
        "stockReversionAction": {
            "type": "string",
            "enum": ["stock_to_unknown_location", "create_stocking_process"]
        },
        "associations": {
            "type": "object"
        }
    },
    "definitions": {
        "uuid": {
            "description": "A UUID in HEX format",
            "type": "string",
            "minLength": 32,
            "maxLength": 32,
            "pattern": "^[0-9a-f]{32}$"
        }
    }
}
